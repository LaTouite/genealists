<% if @registre.anneededebut != @registre.anneedefin %>
  <h1><%= @registre.anneededebut + "-" + @registre.anneedefin + ", " + @registre.nature + " (côte: " + @registre.cote + ")" %> </h1>
<% else %>
  <h1><%= @registre.anneededebut + ", " + @registre.nature + " (côte: " + @registre.cote + ")" %> </h1>
<% end %>
<p><%= link_to "Modifier", edit_ville_registre_path(@ville, @registre) %></p>
<p><%= link_to "Supprimer", ville_registre_path(@ville, @registre), method: :delete, data: { confirm: "Voulez-vous vraiment supprimer?"} %></p>
<%= link_to "Tous les Registres de " + @ville.name, ville_registres_path %>
